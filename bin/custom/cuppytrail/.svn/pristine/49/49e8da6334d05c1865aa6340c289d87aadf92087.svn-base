package de.hybris.platform.cuppytrail.daos;

import de.hybris.platform.cuppytrail.model.ForgotPasswordProcessModel;

import java.util.List;


/**
 * DAO to handle ForgotPasswordModels
 */
public interface ForgotPasswordProcessDAO
{

	/**
	 * Returns a list of {@link ForgotPasswordProcessModel}s by key. The List should not contain more than 1 element.
	 * 
	 * @key the key of the process
	 * @return the processes with the given key
	 */
	List<ForgotPasswordProcessModel> findForgotPasswordModelByKey(String key);

}
